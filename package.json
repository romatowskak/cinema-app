{
  "name": "cinema-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "stylelint": "stylelint '**/*.scss'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write './**/*.ts' './**/*.scss' './**/*.html'",
    "sentry:createrelease": "./node_modules/.bin/sentry-cli releases -o sentrymonitoring new -p sentrymonitoring dev",
    "sentry:associatecommits": "./node_modules/.bin/sentry-cli releases -o sentrymonitoring set-commits dev --auto",
    "sentry:uploadsourcemaps": "./node_modules/.bin/sentry-cli releases -o sentrymonitoring -p sentrymonitoring files dev upload-sourcemaps --url-prefix \"~/\" --rewrite --validate dist",
    "sentry:uploadsourcemaps:finalize": "./node_modules/.bin/sentry-cli releases -o sentrymonitoring -p sentrymonitoring finalize dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier --staged && git add . && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@ngrx/effects": "^8.5.1",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.5.1",
    "@ngrx/store-devtools": "^8.5.2",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^6.0.0",
    "@sentry/angular": "^6.17.1",
    "@sentry/browser": "^6.17.2",
    "@sentry/tracing": "^6.17.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "i": "^0.3.6",
    "lodash.camelcase": "^4.3.0",
    "ngrx-store-localstorage": "^12.0.1",
    "ngx-bar-rating": "^2.0.0",
    "ngx-gallery-9": "^1.0.6",
    "ngx-pagination": "^5.1.1",
    "npm": "^7.24.0",
    "rxjs": "~6.6.7",
    "tslib": "^1.10.0",
    "uuidjs": "^4.2.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@sentry/cli": "^1.63.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash.get": "^4.4.2",
    "@types/lodash.sortby": "^4.7.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.4.1",
    "protractor": "~5.4.0",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.21.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.8.3"
  }
}
